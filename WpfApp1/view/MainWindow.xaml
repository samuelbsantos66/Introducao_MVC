        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Margin="0,0,0,-6">
        <Button x:Name="Cadastrar" Content="Cadastrar" HorizontalAlignment="Left" Margin="508,357,0,0" VerticalAlignment="Top" Click="Cadastrar_Click"/>
